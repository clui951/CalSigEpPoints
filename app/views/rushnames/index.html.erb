<header>
    <div class="header-content-new">
        <div class="header-content-new-inner">
        	<p></p>
        	<h1>Submitted Rush Names</h1>
        	<hr>
        	<%= link_to "New Rush Name", rushnames_new_path, :class => "btn btn-primary btn-xl page-scroll" %> <!-- Create a link that appears as "new" that leads to rushnames_new_path -->
        	<hr>
			<% @rushnames.each do |u| %> <!-- Iterates through each name under the variable u -->
			  <p> Name: <%= link_to u.name, rushname_path(u) %>, Email: <%= u.email %></p> <!-- Show the name-->
			<% end %>
			<p></p>
        </div>
    </div>
</header>
