<header>
    <div class="header-content-new">
        <div class="header-content-new-inner">
        	<p></p>
        	<h1> Submit a Reimbursement</h1>
        	<p></p>

			<%= form_for @reimbursement, html: {class: "new"} do |f| %> <!-- Create form for rushname object -->
			  	<p><label for="new">Item: </label><%= f.text_field :item, :class => "new1" %></p> <!-- Create a text field that sends its value under the key name-->
			  	<p><label for="new">Date: </label><%= f.text_field :date, :class => "new1" %></p> <!-- Create a text field that sends its value under the key age -->
			  	<p><label for="new">Cost: </label><%= f.text_field :cost, :class => "new1" %></p>
          <p><label for="new">VP: </label><%= f.text_field :VP, :class => "new1" %></p>
			  	<%= f.submit "Submit Reimbursement", :class => "btn btn-primary btn-xl page-scroll"%><!-- Create submit button-->
			  	<p></p>
			<% end %>

        </div>
    </div>
</header>
