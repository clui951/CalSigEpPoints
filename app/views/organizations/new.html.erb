<header>
    <div class="header-content-new">
        <div class="header-content-new-inner">
        	<p></p>
        	<h1> Create your organization</h1>
        	<p></p>

			<%= form_for @organization, html: {class: "new"} do |f| %> <!-- Create form for organization object -->
			  	<p><label for="new">Organization: </label><%= f.text_field :name, :class => "new1" %></p>
			  	<%= f.submit "Create organization", :class => "btn btn-primary btn-xl page-scroll"%><!-- Create submit button-->
			  	<p></p>
			<% end %>

        </div>
    </div>
</header>