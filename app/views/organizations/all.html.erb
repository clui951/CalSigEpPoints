<header>
    <div class="header-content-new">
        <div class="header-content-new-inner">
        	<p></p>
        	<h1>All Organizations</h1>
        	<hr>
        	<% if not Organization.first %>
        		<p> No Organizations Created </p>
        	<% else %>
        		<% Organization.all.each do |u| %>
        			<%= u.name %>
        		<% end %>
        	<% end %>

        	<hr>
        	<%= link_to "Create New Organization", organizations_new_path, :class => "btn btn-primary btn-xl page-scroll" %> <!-- Create a link that appears as "new" that leads to rushnames_new_path -->
        	<hr>
        	
			<p></p>
        </div>
    </div>
</header>

